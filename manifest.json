{
    "manifest_version": 3,
    "name": "YouTube Speed Toggle",
    "version": "1.0",
    "description": "Toggle YouTube video speed between 1x and 2x.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  